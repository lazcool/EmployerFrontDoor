        <mxCell id="jVz3VWOv9970-iqRbwlF-12" value="Employer Front Door Architecture&lt;br&gt;(Content Updates Delayed Version)" style="text;html=1;resizable=0;autosize=1;align=center;verticalAlign=middle;points=[];fillColor=none;strokeColor=none;rounded=0;fontSize=20;" parent="1" vertex="1">
